public class Time { 
  
 private int hour, minute; 
 
 public Time () { 
  hour = 12; 
  minute = 0; 
 } 
  
 public void addMinutes (int m) { 
       int totalMinutes = 
   ((60*hour) + minute + m) % (24*60); 
    if (totalMinutes<0) 
   totalMinutes = totalMinutes + (24*60); 
    hour = totalMinutes / 60; 
    minute = totalMinutes % 60; 
 } }
